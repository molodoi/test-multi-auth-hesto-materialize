{
    "private": true,
    "scripts": {
        "dev-frontend": "npm run development -- --env.site=frontend",
        "watch-frontend": "npm run watch-poll -- --env.site=frontend",
        "hot-frontend": "npm run hot -- --env.site=frontend",
        "prod-frontend": "npm run production -- --env.site=frontend",
        "dev-admin": "npm run development -- --env.site=admin",
        "watch-admin": "npm run watch-poll -- --env.site=admin",
        "hot-admin": "npm run hot -- --env.site=admin",
        "prod-admin": "npm run production -- --env.site=admin",
        "dev-school": "npm run development -- --env.site=school",
        "watch-school": "npm run watch-poll -- --env.site=school",
        "hot-school": "npm run hot -- --env.site=school",
        "prod-school": "npm run production -- --env.site=school",
        "dev-renter": "npm run development -- --env.site=renter",
        "watch-renter": "npm run watch-poll -- --env.site=renter",
        "hot-renter": "npm run hot -- --env.site=renter",
        "prod-renter": "npm run production -- --env.site=renter",
        "dev": "npm run development",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run development -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run production",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
    },
    "devDependencies": {
        "axios": "^0.18",
        "bootstrap": "^4.0.0",
        "browser-sync": "^2.24.4",
        "browser-sync-webpack-plugin": "^2.0.1",
        "cross-env": "^5.1",
        "jquery": "^3.2",
        "laravel-mix": "^2.0",
        "lodash": "^4.17.4",
        "materialize-css": "^0.100.2",
        "popper.js": "^1.12",
        "vue": "^2.5.7"
    }
}
